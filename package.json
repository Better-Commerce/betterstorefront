  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css}": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@assets/css/*": [
        "assets/css/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@old-components/*": [
        "old-components/*"
      ],
      "@components/*": [
        "new-components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "new-components",
      "old-components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
