{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "node generate-locales.js && cross-env NODE_OPTIONS='--inspect' next dev",
    "dev-windows": "node generate-locales.js && cross-env NODE_OPTIONS='--inspect=5009' next dev",
    "build": "node generate-locales.js && next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "generate": "graphql-codegen",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js"
  },
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@engineerapart/nextscript": "^2.0.2",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^2.0.14",
    "@react-spring/web": "^9.6.1",
    "@stripe/react-stripe-js": "^1.16.4",
    "@stripe/stripe-js": "^1.46.0",
    "@types/qs": "^6.9.7",
    "@types/react-gtm-module": "^2.0.1",
    "@types/store": "^2.0.2",
    "@vercel/fetch": "^6.2.0",
    "@xmldom/xmldom": "^0.8.6",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "email-validator": "^2.0.4",
    "formik": "^2.2.9",
    "fs": "^0.0.1-security",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.8.5",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "memory-cache": "^0.2.0",
    "next": "^13.1.6",
    "next-seo": "^5.15.0",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.16",
    "postcss-nesting": "^10.2.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.1.4",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.3.1",
    "react-image-zoom": "^1.3.1",
    "react-image-zooom": "^1.0.4",
    "react-infinite-scroll-component": "^6.1.0",
    "react-merge-refs": "^1.1.0",
    "react-paginate": "^7.1.3",
    "react-public-ip": "^1.0.0",
    "react-use-measure": "^2.0.4",
    "store": "^2.0.12",
    "stripe": "^11.8.0",
    "swell-js": "^3.19.5",
    "swiper": "^8.4.7",
    "swr": "^0.5.6",
    "tabbable": "^6.0.1",
    "tailwindcss": "^2.2.2",
    "uuidv4": "^6.2.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/schema-ast": "^2.6.1",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@next/bundle-analyzer": "^13.1.6",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.random": "^3.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.8",
    "deepmerge": "^4.3.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "graphql": "^16.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.8.3",
    "typescript": "4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@assets/css/*": [
        "assets/css/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
